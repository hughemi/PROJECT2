{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": "container",
  "height": 350,
  "title": { "text": "Average Double Vaccination Rates of all UN Subregions"},
  "data": {
      "url": "https://raw.githubusercontent.com/hughemi/PROJECT2/main/subregion_double.csv"
  },
  "mark": "bar",
"encoding": {
  "x": {
      "field": "subregion",
      "type": "nominal",
      "scale": {"padding": 0.1},
      "sort": "y",
      "title": "Subregion"
    },
  "y": {
      "field": "ave_douvble_vax",
      "type": "quantitative",
      "title": "Population double vaccinated (%)"
    },
  "color": {
      "field": "region",
      "scale": {
          "domain": [
          "East Asia & Pacific",
          "Europe & Central Asia",
          "Latin America & Caribbean",
          "Middle East & North Africa",
          "North America",
          "South Asia",
          "Sub-Saharan Africa"
          ],
          "range": [
          "#66c2a5",
          "#fc8d62",
          "#e5c494",
          "#e78ac3",
          "#a6d854",
          "#ffd92f",
          "#1f78b4"
          ]
        }
    },
  "opacity":{
    "field": "Sub-Saharan Africa",
    "legend": null
  },
  "tooltip": [
    {"field": "region", "type": "nominal", "title": "Region"},
    {"field": "subregion", "type": "nominal", "title": "Subregion"},
    {"field": "ave_douvble_vax", "type": "nominal", "title": "Population double vaccinated (%)"}
  ]
}
}